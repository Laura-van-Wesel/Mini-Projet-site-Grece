		/* Tout le CSS suivant concerne l'infobulle */
			
			/* Rend invisible tout notre bloc span */
			a.info_bulle span
			{								   
				position       :  absolute;
				top            :  -2000em;
				left           :  -2000em;
				width          :  1px;
				height         :  1px;
				overflow       :  hidden; 
				background     :  #d9fbbe;   /* Définition du fond, sinon on a le même que notre lien #DDEEFF */
				border         :  1px solid #0ebe26; /* Définition des bordures */
			} 

			a.info_bulle
			{				
			  color           : #04300a;
			  
			  text-decoration : none; 
			  
			  padding         : 2px 16px 2px 2px; /*Définition des marges intérieures de notre lien */
			  
			  /* Définition de l'arrière plan de notre lien */
			  background      : transparent url('images/interrogation.gif') no-repeat right center;
			  
			  position        : absolute; /* Indispensable pour le bon positionnement de l'info-bulle, avant il y avait relative */ 
			}  
			 
			a.info_bulle:hover 
			{
			   border          : 0;   ligne qui corrige le bug d'IE6 et inférieur */
			   background: none; /* correction d'un bug IE */
			}  

			/* Rend visible tout notre bloc span et lui attribue une taille */
			a.info_bulle:hover span, a.info_bulle:focus span, a.info_bulle:active span 
			{
				padding         : 16px;
				top        :  auto;
				left       :  auto;
				width      :  350px;
				height     :  auto;
				overflow   :  visible;
			}
